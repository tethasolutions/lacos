{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "lacos": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "i18n": {
                "locales": {
                    "it": {
                        "translation": "src/locale/messages.it.xlf",
                        "baseHref": ""
                    }
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/lacos",
                        "i18nMissingTranslation": "error",
                        "baseHref": "",
                        "localize": [
                            "it"
                        ],
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "node_modules/@progress/kendo-theme-bootstrap/dist/all.css",
                            "node_modules/bootstrap/dist/css/bootstrap.css",
                            "node_modules/@fortawesome/fontawesome-free/css/all.css",
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "allowedCommonJsDependencies": [
                            "@progress/kendo-angular-intl/locales/it/all"
                        ]
                    },
                    "configurations": {
                        "it": {
                            "aot": true,
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ]
                        },
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "outputHashing": "all",
                            "extractLicenses": true
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "lacos:build:production"
                        },
                        "development": {
                            "browserTarget": "lacos:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "lacos:build"
                    }
                }
            }
        }
    },
    "cli": {
      "analytics": false
    }
}
